        "# Analyzing the STATUS variable\n",
        "\n",
        "# Group by STATUS and sum the SALES\n",
        "sales_by_status = df.groupby('STATUS')['SALES'].sum().reset_index()\n",
        "\n",
        "# Calculate the total sales\n",
        "total_sales = df['SALES'].sum()\n",
        "\n",
        "# Calculate the percentage contribution of each status\n",
        "sales_by_status['Percentage'] = (sales_by_status['SALES'] / total_sales) * 100\n",
        "\n",
        "# Display the results\n",
        "print(sales_by_status)"
      ],
