      "cell_type": "code",
      "source": [
        "# 3. Regional Growth Analysis: Compare sales performance across different regions over time\n",
        "# Extract year and quarter from ORDERDATE\n",
        "df['Year'] = df['ORDERDATE'].dt.year\n",
        "df['Quarter'] = df['ORDERDATE'].dt.quarter\n",
        "\n",
        "# Group by region (e.g., country), year, and quarter and sum up sales\n",
        "region_sales = df.groupby(['COUNTRY', 'Year', 'Quarter'])['SALES'].sum().reset_index()\n",
        "\n",
        "# Calculate growth rates\n",
        "region_sales['Sales Growth'] = region_sales.groupby('COUNTRY')['SALES'].pct_change()\n",
        "\n",
        "# Visualize the results\n",
        "plt.figure(figsize=(12, 6))\n",
        "sns.lineplot(data=region_sales, x='Year', y='Sales Growth', hue='COUNTRY', marker='o', palette='pastel')\n",
        "plt.title('Regional Sales Growth Over Time')\n",
        "plt.xlabel('Year')\n",
        "plt.ylabel('Sales Growth Rate')\n",
        "plt.xticks(region_sales['Year'].unique())\n",
        "plt.grid(True)\n",
        "plt.legend(title='Country')\n",
        "plt.show()"
      ],
